<h1>Businesses</h1>

<table>
  <tr>
    <th>Business type</th>
    <th>Name</th>
    <th>Description</th>
    <th>Address</th>
    <th>Tag line</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @businesses.each do |business| %>
    <tr>
      <td><%= business.business_type.name %></td>
      <td><%= business.name %></td>
      <td><%= business.description %></td>
      <td><%= business.address %></td>
      <td><%= business.tag_line %></td>
      <td><%= link_to 'Details', customer_business_path(business) %></td>
    </tr>
  <% end %>
</table>

